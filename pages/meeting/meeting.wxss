.meeting-img {
  width: 100px; 
  height: 100px;
  border-radius: 50%;
  border: 5rpx solid white;
}

.meeting-profile {
  width: 100px; 
  text-align: center;
}

.wrapper {
  width: 100%;
  background-color: #4BDDB9;
  height: 300rpx;
}

.container-meeting {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: -100rpx;
} 

.box {
  margin: 0 20rpx;
  padding: 20rpx;
  line-height: 1.5em;
  text-align: center;
}

.call {
  margin-top: 100rpx;
}