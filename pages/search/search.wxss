.box {
  margin: 80rpx 30rpx;
  padding: 20rpx;
  line-height: 1.5em;
  text-align: center;
  /*box-shadow:1rpx 1rpx 30rpx #dde0e1;*/
}

.tags {
  border: 1rpx solid #4BDDB9;
  box-shadow:1rpx 1rpx 20rpx #c7f6eb;
  border-radius: 40rpx;
  letter-spacing: 0.4px;
  padding: 15rpx 20rpx;
  margin: 10rpx;
}

.container-tags {
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  margin: 0 20rpx;
}