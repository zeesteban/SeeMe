<import src="../common/head.wxml" />
<import src="../common/foot.wxml" />

<view class="container">


  <view class="page-body">
    <form bindsubmit="formSubmit" catchreset="formReset">


      <view class="page-section">
        <view class="page-section-title">Bio</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd">
              <input class="weui-input" name="bio" placeholder="Edit your bio" />
            </view>
          </view>
        </view>
      </view>


      <view class="page-section">
        <view class="page-section-title">Hobbies & Interests</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd">
              <input class="weui-input" name="hobby" placeholder="It is a input " />
            </view>
          </view>
        </view>
      </view>

      <view class="btn-area">
        <button type="primary" formType="submit">Submit</button>
      </view>
    </form>
  </view>

</view>
